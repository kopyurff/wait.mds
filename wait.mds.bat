:internal.getResultFromBuffer - gets result echoed by buffered task.
:::                      %~1 == external variable name with "name" of echoed result to the left of delimeter "=";
:::                      %~2 == external variable name with with "value" of echoed result to the right of delimeter "=".
:::
 (if not defined %~1 (exit /b 1))&(if not defined %~2 (exit /b 1))
 call set %%~1=%%%~1:"=%%
 call set wds_grfb_name="%%%~1:~0,10%%"
 if not %wds_grfb_name%=="wds_result" (set wds_grfb_name=&exit /b 1)
 call set wds_grfb_name=%%%~1:~0,15%%
 call set "wds_grfb_aset=%%%~2%%"
 if not defined wds_grfb_aset (set wds_grfb_name=&set wds_grfb_aset=&exit /b 1)

 (set /a "wds_grfb_exco=0")&(set /a "wds_grfb_getres=0")
 if "[%wds_grfb_name:~-4%]"=="[name]" (
  set "wds_result_name=%wds_grfb_aset%"&set "wds_result_type="&set "wds_result_code="&set "wds_result_echo="
 ) else if "[%wds_grfb_name%]"=="[wds_result_type]" (
  set "wds_result_type=%wds_grfb_aset%"
 ) else if "[%wds_grfb_name%]"=="[wds_result_code]" (
  set "wds_result_code=%wds_grfb_aset%"
 ) else if "[%wds_grfb_name%]"=="[wds_result_echo]" (
  set "wds_result_echo=%wds_grfb_aset%"
 ) else if "[%wds_grfb_name%]"=="[wds_result_aset]" (
  set /a "wds_grfb_getres=1"
  if "[%wds_result_code%]"=="[]" (set /a "wds_grfb_exco=2"&goto:internal.wds_grfb_lunset)
  if "[%wds_result_echo%]"=="[]" (set /a "wds_grfb_exco=2"&goto:internal.wds_grfb_lunset)
  if "[%wds_grfb_aset:~0,1%%wds_grfb_aset:~-1,1%]"=="[""]" (set wds_grfb_aset=%wds_grfb_aset:~1,-1%) else (set /a "wds_grfb_exco=2"&goto:internal.wds_grfb_lunset)
 ) else (
  set /a "wds_grfb_exco=2"&goto:internal.wds_grfb_lunset
 )
 if %wds_grfb_getres% EQU 0 (goto:internal.wds_grfb_lexit)
  if "[%wds_result_type%]"=="[string]" (
   if defined wds_grfb_aset (
    if "%wds_result_code%"=="yes" (
