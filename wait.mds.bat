  (if defined wds_wmc_fnm ((call cd /d %%wds_wmc_fnm%%)&set "wds_wmc_fnm="))
  for /F "usebackq tokens=1,* delims==" %%b in (`echo %%a`) do (
   set "wds_br_b=%%b"
   set "wds_br_c=%%c"
   call:internal.getResultFromBuffer wds_br_b, wds_br_c && echo.>nul || echo %%a
  )
