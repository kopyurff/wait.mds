   if "[%wds_result_type%]"=="[digit]" (
    set /a "%wds_result_name%=%wds_grfb_aset%" >NUL 2>&1 && echo.>nul || set /a "wds_grfb_exco=2"
   ) else (
    set /a "wds_grfb_exco=2"
   )
  )
  :internal.wds_grfb_lunset
  set "wds_result_type="&set "wds_result_code="&set "wds_result_echo="
