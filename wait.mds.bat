     call set wds_grfb_aset=%%wds_grfb_aset:""="%%
     call set wds_grfb_aset=%%wds_grfb_aset:"^"=""%%
    )
   )
  ) else if "%wds_result_code%"=="yes" (set /a "wds_grfb_exco=2"&goto:internal.wds_grfb_lunset)
  
  if "%wds_result_echo%"=="yes" (
   echo %wds_result_name%=%wds_grfb_aset%
  ) else (
   if "[%wds_result_type%]"=="[string]" (
    if defined wds_grfb_aset (
     set "%wds_result_name%=%wds_grfb_aset%" >NUL 2>&1 || set /a "wds_grfb_exco=2"
    ) else (
     set "%wds_result_name%="
    )
    goto:internal.wds_grfb_lunset
