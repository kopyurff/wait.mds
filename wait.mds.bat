::         :newFileName - finds a new name for an object that is not present in the specified folder.
::                        %~1 == name of external variable to set string of found name in format: `%~3digit%~4`;
::                        %~2 == the path where new file should be created (string value or variable name), "" for %TEMP%;
::                        %~3 == prefix for file name;
::                        %~4 == suffix for file name, including extension;
::             Notes. #1: it verifies absence of file with new name by 2 checks with timeout 75 msec;
::                    #2: the procedure tries to find a new name with a minimum serial number using binary search;
::                    #3: see also the macro `@obj_newname`, which searches for a new name using a random number generator.
::
:newFileName
 call:internal.readParameters wds_nfn, %1, %2, %3, %4
 call:internal.initializeMacrosA
 for /L %%a in (1,1,4) do (
  %@mac_check% @str_isempty,wds_nfn_%%a 1:1,wds_nfn_%%a %@istrue% && if %%a NEQ 2 (%@error% %~0 "Wrong parameter #%%a") || echo.>nul
 )
 if defined %wds_nfn_2% call set wds_nfn_2=%%%wds_nfn_2%:"=%%
 for /L %%a in (2,1,4) do call set wds_nfn_%%a="%%wds_nfn_%%a%%"
 if %wds_nfn_2%=="" if exist "%TEMP%" (set wds_nfn_2="%TEMP%") else (set wds_nfn_2="%~d0%~p0")
 if exist %wds_nfn_2% (set wds_nfn_2="%wds_nfn_2:~1,-1%\%wds_nfn_3:~1,-1%") else (%@error% %~0 "Not valid path")
 (set wds_nfn_cnt=65536)&(set wds_nfn_snv=65535)
 for /L %%a in (1,1,17) do (
  (set "wds_nfn_snm=00000!wds_nfn_snv!")&(set wds_nfn_snm="%wds_nfn_2:~1,-1%!wds_nfn_snm:~-6,6!%wds_nfn_4:~1,-1%")
  set /a wds_nfn_cnt=!wds_nfn_cnt!/2
  if exist !wds_nfn_snm! (set /a wds_nfn_snv+=!wds_nfn_cnt!) else (set /a wds_nfn_snv-=!wds_nfn_cnt!)
 )
 if !wds_nfn_snv! EQU 0 set /a wds_nfn_snv+=1
 set wds_nfn_cnt=1
 for /L %%a in (1,1,18) do (
  (set "wds_nfn_res=00000!wds_nfn_snv!")&(set wds_nfn_snm="%wds_nfn_2:~1,-1%!wds_nfn_res:~-6,6!%wds_nfn_4:~1,-1%")
  if not exist !wds_nfn_snm! (
   pathping 127.0.0.1 -n -q 1 -p 75 >NUL 2>&1
   if not exist !wds_nfn_snm! (
    set wds_nfn_res=%wds_nfn_3:~1,-1%!wds_nfn_res:~-6,6!%wds_nfn_4:~1,-1%&goto:internal.wds_nfn_report
   )
  )
  call set /a wds_nfn_snv+=%%wds_nfn_cnt%%
  call set /a wds_nfn_cnt=%%wds_nfn_cnt%%*2
 )
 %@error% %~0 "Failed to find non-existent file name"
 :internal.wds_nfn_report
 if %wds_nfn_report% EQU 1 (
  call:internal.addResultToReport %wds_nfn_1%, wds_nfn_res, string, asis
 ) else (
  set %wds_nfn_1%=%wds_nfn_res%
  %@unset% 1:wds_nfn_
 )
exit /b 0
